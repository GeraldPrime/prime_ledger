{
    "builds":[{
        "src":"coin/wsgi.py",
        "use":"@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.9",
        "command": "pip install -r requirements.txt && python manage.py collectstatic --noinput"

    }
    }
   
],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles_build/$1"
        },
        
        {
            "src": "/(.*)",
            "dest": "coin/wsgi.py"
        }
        
    ]

}